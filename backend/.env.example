# Server Configuration
PORT=3000
NODE_ENV=development

# Database
DATABASE_PATH=./cart_jobs.db

# Proxy Configuration (optional - helps avoid IP blocking)
# Get proxies from services like BrightData, Oxylabs, or SmartProxy
PROXY_HOST=
PROXY_PORT=
PROXY_USERNAME=
PROXY_PASSWORD=

# Webhook Configuration
WEBHOOK_SECRET=your-webhook-secret-key-here

# Rate Limiting
MAX_CONCURRENT_JOBS=5
JOB_TIMEOUT_MS=120000

# Feature Flags
ENABLE_WALMART=true
ENABLE_INSTACART=false
ENABLE_TARGET=false

# Walmart Affiliate API (Recommended - replaces Puppeteer scraping!)
# Sign up: https://affiliates.walmart.com/
WALMART_PUBLISHER_ID=
WALMART_API_KEY=

# Logging
LOG_LEVEL=info
